{

    "builds": [{

        "src": "nba_data_project/wsgi.py",

        "use": "@vercel/python",

        "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }

    }],

     
    "functions": {
        
        "pages/**/*": {
          "excludeFiles": "virtual/**"
        }
    },

    "routes": [

        {

            "src": "/(.*)",

            "dest": "nba_data_project/wsgi.py"

        }

    ]

}
