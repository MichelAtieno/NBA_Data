{

    "builds": [{

        "src": "nba_data_project/wsgi.py",

        "use": "@vercel/python",

        "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" },

        "api/**/*.py": {
            "excludeFiles": "public/**"
          }
          
    }],

    "routes": [

        {

            "src": "/(.*)",

            "dest": "nba_data_project/wsgi.py"

        }

    ]

}
